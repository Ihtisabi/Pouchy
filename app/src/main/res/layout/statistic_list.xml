            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="1,000,000"
            android:textSize="16sp"
            android:textStyle="bold"
            android:textColor="@android:color/holo_green_dark"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <!-- Bagian sebelas (transfer) -->
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/transferLayout"
        app:layout_constraintTop_toBottomOf="@id/investmentLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="8dp"
        android:layout_marginTop="5dp"
        android:background="@drawable/border_transaksi">

        <!-- Icon ImageView -->
        <ImageView
            android:id="@+id/iconTransfer"
            android:layout_width="48dp"
            android:layout_height="48dp"
            android:src="@drawable/transfer_icon"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:contentDescription="icon" />

        <!-- Title Text (e.g., Salary or Beauty) -->
        <TextView
            android:id="@+id/Transfer"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="Transfer"
            android:textStyle="bold"
            android:textSize="16sp"
            android:textColor="#5B5858"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toEndOf="@id/iconTransfer"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" />

        <!-- Amount Text (e.g., +1,000,000 or -100,000) -->
        <TextView
            android:id="@+id/amountTransfer"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="1,000,000"
            android:textSize="16sp"
            android:textStyle="bold"
            android:textColor="@android:color/holo_green_dark"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
